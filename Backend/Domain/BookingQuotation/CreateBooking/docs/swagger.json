{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "This microservice creates a booking in SQL Server.",
        "title": "Create Booking API",
        "termsOfService": "http://example.com/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.example.com/support",
            "email": "support@example.com"
        },
        "license": {
            "name": "Apache 2.0",
